<?xml version="1.0"?>
<fragment version="1">

  <install>
    <!-- Unzip the contents of our web application into the server -->
    <unzip from="${bundle.fileName}" to="/" prefix="web">
      <include>web/**</include>
    </unzip>
    <append from="i18n/messages.json" to="nuxeo.war/ui/i18n/messages.json" addNewLine="true"/>
  </install>

</fragment>
